EXPO_PUBLIC_ADMOB_IOS_APP_ID=ca-app-pub-xxxxx~xxxxx
EXPO_PUBLIC_ADMOB_BANNER_ANDROID=ca-app-pub-xxxxx/xxxxx
EXPO_PUBLIC_ADMOB_BANNER_IOS=ca-app-pub-xxxxx/xxxxx
EXPO_PUBLIC_ADMOB_INTERSTITIAL_ANDROID=ca-app-pub-xxxxx/xxxxx
EXPO_PUBLIC_ADMOB_INTERSTITIAL_IOS=ca-app-pub-xxxxx/xxxxx

# App Configuration
EXPO_PUBLIC_APP_ENV=development
